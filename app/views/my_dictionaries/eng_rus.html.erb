<table class="table table-bordered table-striped ">
  <thead>
    <tr>
      <th>ID</th>
      <th>Eng</th>
      <th>Rus</th>
    </tr>
  </thead>
  <tbody>
    <th><%=@my_dictionary.id%></th>
    <th><%=@my_dictionary.eng%></th>
    <th id="translation"></th>
  </tbody>
</table>
<p>
<input type='button' value='translation' class="btn btn-info" onClick="translation()">
</p>
<%= link_to 'Eng - Rus', eng_rus_lesson_my_dictionaries_path, class: "btn btn-info" %>

<script >
  function translation(){
    document.getElementById('translation').innerHTML = "<%=@my_dictionary.rus%>"
  }
</script >